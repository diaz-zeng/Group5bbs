<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.soft863.group5.bbs.dao.UserMapper" >


<resultMap id="userMap" type="user">
    <id property="id" column="user_id" javaType="java.lang.Integer"></id>
    <result property="name" column="user_name" javaType="java.lang.String"></result>
    <result property="photo" column="user_photo"></result>
    <result property="password" column="user_password"></result>
    <result property=""
</resultMap>
    <select id="queryAllPoint" resultMap="">
        SELECT p.point_id,p.point_integral,p.point_date,p.point_source,p.point_id,u.user_id
        FROM pointRecord p,user u
        WHERE p.point_id=u.user_id AND u.user_id=
    </select>

    <select id="queryAllUserName" resultType="user">
        SELECT user_name name FROM user
    </select>

</mapper>